.row
  .col-lg-12
    .page-header
      %h1
        = "#{@user.name}'s"
        %small Oauth Access Tokens
.row
  .col-lg-12
    %table{class: 'table table-striped table-hover'}
      %thead
        %tr
          - @oauth_access_tokens.columns.each do |column|
            %th
              = column.name.camelize
      %tbody
        - @oauth_access_tokens.each do |oauth_access_token|
          %tr
            - @oauth_access_tokens.columns.each do |column|
              %td
                = oauth_access_token[column.name]



